<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="unisave2006.grabber">
  <subclass name="DoubleGrabber" extends="AbstractGrabber" discriminator-value="DOUB">
  	<many-to-one name="grab1" class="GrabberInterface"></many-to-one>
  	<many-to-one name="grab2" class="GrabberInterface"></many-to-one>
  </subclass>
</hibernate-mapping>